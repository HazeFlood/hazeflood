set smartindent
set tabstop=4
set softtabstop=4
set shiftwidth=4
set nu
set history=10000
set whichwrap+=h,l,<,>
set expandtab
set clipboard+=unnamed
set viminfo+=!
set incsearch
set ignorecase
set smartcase
set scrolloff=5
set undofile
set list
set filetype=c
syntax on
highlight Function cterm=bold, ctermbg=red   


